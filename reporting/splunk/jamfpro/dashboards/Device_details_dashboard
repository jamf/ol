      <search>
        <query>sourcetype="jssUapiComputer:app" computer_meta.id="$jssID$" | dedup computer_meta.assignedUser | table computer_meta.assignedUser</query>
        <earliest>-24h@h</earliest>
        <latest>now</latest>
      </search>
