      <choice value="*">Any</choice>
      <search>
        <query>sourcetype="jssUapiComputer:*" computer_meta.id="*" computer_meta.reportDate="*" computer_meta.assignedUser="$assignedUser$" computer_meta.eventID="*" | stats count by computer_meta.id</query>
        <earliest>-7d@h</earliest>
        <latest>now</latest>
      </search>
      <fieldForLabel>computer_meta.id</fieldForLabel>
      <fieldForValue>computer_meta.id</fieldForValue>
